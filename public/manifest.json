{
  "name": "NewsEz",
  "description": "뉴스 이해에 도움을 주기 위한 어플리케이션",
  "manifest_version": 3,
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html"
  }
}
